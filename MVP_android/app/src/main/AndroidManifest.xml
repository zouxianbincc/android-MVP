<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.example.cc">

    <application
        android:name=".CCApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
            android:name="com.example.cc.UILayer.Activity.Start.StartActivity"
            android:label="@string/title_activity_start"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>

        <activity android:name="com.example.cc.UILayer.Activity.Guide.GuideActivity"></activity>
        <activity android:name="com.example.cc.UILayer.Activity.Index.IndexActivity"></activity>
        <activity android:name="com.example.cc.UILayer.Activity.Login.ForgetPassWordActivity"></activity>
        <activity android:name="com.example.cc.UILayer.Activity.Login.LogInActivity"></activity>
    </application>

</manifest>